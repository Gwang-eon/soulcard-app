{
  "version": 2,
  "builds": [
    {
      "src": "public-dev/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public-dev/$1"
    }
  ],
  "env": {
    "NODE_ENV": "development",
    "VERCEL_ENV": "development",
    "ENABLE_DEBUG": "true"
  }
}